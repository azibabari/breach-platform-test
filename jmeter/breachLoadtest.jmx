          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">vars.put(&quot;email&quot;, &quot;user${counter}_${__time(yyyyMMddHHmmssSSS)}@yopmail.com&quot;);</stringProp>
        </BeanShellPreProcessor>
